<h1> Edit</h1>
<form action="/Review/save" method="post">
    <input type="hidden" name="review_id" value="<%= onerec.review_id %>" >
<table>
    <tr> <td> Review ID: </td> <td> <input type="number" name="review_id" value="<%= onerec.review_id %>" required> </td> </tr>
    <tr> <td> Product ID: </td> <td> <input type="number" name="product_id" value="<%= onerec.product_id %>" required> </td> </tr>
    <tr> <td> Customer ID: </td> <td> <input type="number" name="cusomer_id" value="<%= onerec.customer_id %>" required> </td> </tr>
    <tr> <td> Submit Date: </td> <td> <input type="date" name="submitdate" value="<%= onerec.submitdate %>" required> </td> </tr>
    <tr> <td> Description: </td> <td> <input type="text" name="description" value="<%= onerec.description %>" required> </td> </tr>
    <tr> <td> Rating: </td> <td> <input type="number" name="rating" value="<%= onerec.rating %>" required> </td> </tr>
</table>
<button type="submit">Save</button>
</form>